<script>
    $(document).ready(function() {
        //MyValidator
        var config = {
            rules : {
                username : {
                    required : true
                },
                email : {
                    required : true
                },
                password : {
                    required : true
                },
                confirmpassword : {
                    required : true,
                    equalTo : "#password"
                }
            },
            messages : {
                username : {
                    required : "请输入注册用户名."
                },
                email : {
                    required : "请输入注册邮箱."
                },
                password : {
                    required : "请输入注册密码."
                },
                confirmpassword : {
                    required : "请输入确认密码.",
                    equalTo: "两次密码输入不一致"
                }
            },
            ajaxSubmit : {
                url : "/register"
            }
        };
        MyValidator.init($("#register_form"),config);
    });
</script>
